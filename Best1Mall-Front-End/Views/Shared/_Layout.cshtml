<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="/assets/css/swiper-bundle.min.css">
    <link rel="stylesheet" href="/assets/css/jquery-ui.css">
    <link rel="stylesheet" href="/assets/css/select2.min.css">
    <link rel="stylesheet" href="/assets/css/lightgallery-bundle.css" />
    <link rel="stylesheet" href="/assets/css/main.min.css">
    <link rel="stylesheet" href="/custom/site.css">

    <link href="https://cdnjs.cloudflare.com/ajax/libs/lightbox2/2.11.3/css/lightbox.min.css" rel="stylesheet" />


    <title>@ViewData["Title"]</title>
    <link type="image/x-icon" href="~/images/img-search.png" rel="shortcut icon" />
    <!-- CSS-->

</head>
<body>
    <partial name="_Header" />
    <partial name="_Login" />

    <section id="main">
        @RenderBody()
    </section>
    <partial name="_Footer" />
    <button class="backToTopBtn"></button>
    <partial name="_Lightbox" />

    <script asp-append-version="true" src="/assets/js/jquery.min.js"></script>
    <script asp-append-version="true" src="/assets/js/swiper-bundle.min.js"></script>
    <script asp-append-version="true" src="/assets/js/jquery-ui.js"></script>
    <script asp-append-version="true" src="/assets/js/common.js"></script>
    <script asp-append-version="true" src="/custom/jquery.magnific-popup.min.js"></script>
    <script asp-append-version="true" src="/assets/js/select2.min.js"></script>
    <script src="/modules/constants.js" asp-append-version="true"></script>
    <script src="/modules/services.js" asp-append-version="true"></script>
    <script src="/modules/global.js" asp-append-version="true"></script>

    <!-- lightgallery-->
    <script src="/assets/js/lightgallery.min.js"></script>
    <script src="/assets/js/video.min.js"></script>
    <script src="/assets/js/lightgallery.umd.js"></script>
    <script src="/assets/js/lg-video.umd.js"></script>
    <script src="/assets/js/lg-thumbnail.min.js"></script>
    <!-- end lightgallery-->

    <!-- Modules-->
    <script asp-append-version="true" src="/modules/account.js"></script>
    <script asp-append-version="true" src="/modules/home_product.js"></script>
    <script asp-append-version="true" src="~/modules/support.js"></script>
    <script type="text/javascript" src="~/modules/feedback.js" asp-append-version="true"></script>
    <script src="https://accounts.google.com/gsi/client" async></script>
    @* <script async defer crossorigin="anonymous" src="https://connect.facebook.net/vi_VN/sdk.js#xfbml=1&version=v20.0" nonce="P6zkdA3O"></script> *@

    <script src="https://cdnjs.cloudflare.com/ajax/libs/lightbox2/2.11.3/js/lightbox.min.js"></script>
    


    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
