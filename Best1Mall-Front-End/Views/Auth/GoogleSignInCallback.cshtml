@{
    Layout = null;
    string data = (string)ViewBag.Data;
    string msg = (string)ViewBag.msg;
}
<!DOCTYPE html>
<html>
<head>
    <title>Đang xử lý đăng nhập Google...</title>
</head>
<body>
    <script>
        
        document.addEventListener('DOMContentLoaded', function () {
            var data = '@Html.Raw(data)'
            if(data!=null && data.trim()!=''){
                localStorage.setItem('account', '@Html.Raw(data)');
            }
            var msg = '@Html.Raw(msg)'
            if (msg != null && msg.trim() != '') {
                sessionStorage.setItem('msg', '@Html.Raw(msg)');
            }
            window.close();
        });
    </script>
</body>
</html>
